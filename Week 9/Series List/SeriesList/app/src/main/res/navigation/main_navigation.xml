<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.l0122147.salsarizki.serieslist.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_seriesActivity"
            app:destination="@id/seriesActivity" />
    </fragment>
    <activity
        android:id="@+id/seriesActivity"
        android:name="com.l0122147.salsarizki.serieslist.SeriesActivity"
        android:label="activity_series"
        tools:layout="@layout/activity_series" />
</navigation>